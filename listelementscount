my_lt=[]
for i in range(6):
    num=int(input("Enter elements:"))
    my_lt.append(num)
    sum=0
    for i in my_lt:
        sum=sum+i
        print(sum)
or,
my_lt=[]
my_range= int(input("Enter Range:"))
for i in range(my_range):
    num=int(input("Enter elements:"))
    my_lt.append(num)
    sum=0
    for i in my_lt:
        sum=sum+i
        print(sum)
